QPushButton {
  background-position: center;
  background-repeat: no-reperat;
  border: none;
}
QFrame#barreDActions_frame{
  background-color: transparent;
}

QPushButton#home {
  background-image: url(icons/16x16/home-3-line.png);
  background-color: transparent;
  background-position: left center;
  color: white;
  border-left: 28px solid rgb(30, 30, 30);
  text-align: left;
  padding-left: 45px;
}
QPushButton:hover#home {
  background-color: rgb(35, 35, 35);
  border-left: 28px solid rgb(35, 35, 35);
}
QPushButton:pressed#home {
  background-color: rgb(45, 45, 45);
  border-left: 28px solid rgb(45, 45, 45);
}

